{
    "routes": [
        {
            "src": "/",
            "dest": "/public/html/index.html"
        },
        {
            "src": "/draft.html",
            "dest": "/public/html/draft.html"
        },
        {
            "src": "/api/",
            "dest": "/api/index.js"
        },
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        },
        {
            "src": "/api/teams",
            "dest": "/api/teams.json"
        },
        {
            "src": "/api/players",
            "dest": "/api/players.json"
        }
    ]
}