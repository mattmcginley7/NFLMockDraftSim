{
    "routes": [
        {
            "src": "/",
            "dest": "/public/html/index.html"
        },
        {
            "src": "/draft.html",
            "dest": "/public/html/draft.html"
        },
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ]
}